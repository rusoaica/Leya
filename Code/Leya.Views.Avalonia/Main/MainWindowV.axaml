    <Image Source="avares://Leya.ViewsAvalonia/Assets/background.jpg" IsVisible="False" Margin="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="NaN" Height="NaN" Stretch="UniformToFill"/>
